{
  "name": "boilerplate-react-webpack",
  "version": "0.1.1",
  "description": "A webpack React starter",
  "repository": "https://github.com/dev-academy-challenges/boilerplate-react-webpack.git",
  "main": "server/index.js",
  "scripts": {
    "postinstall": "webpack && knex migrate:latest",
    "dev": "nodemon --watch server & webpack --watch",
    "start": "node server/index",
    "test": "jest",
    "watch": "nodemon -e js,jsx -x npm test",
    "h:deploy": "git push heroku master",
    "h:migrate": "heroku run knex migrate:latest",
    "h:rollback": "heroku run knex migrate:rollback",
    "h:seed": "heroku run knex seed:run",
    "knex": "knex",
    "node": "8.10.0"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "author": "EDA",
  "license": "ISC",
  "devDependencies": {
    "enzyme": "^2.8.2",
    "jest": "^23.4.2",
    "nodemon": "^1.18.3",
    "sqlite3": "^4.0.2",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "cors": "^2.8.3",
    "express": "^4.14.0",
    "jsdom": "^11.12.0",
    "jsx-loader": "^0.13.2",
    "knex": "^0.13.0",
    "nock": "^9.6.0",
    "pg": "^7.4.3",
    "react": "^15.5.4",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-responsive-modal": "^3.2.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "save": "^2.3.2",
    "superagent": "^2.2.0",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
